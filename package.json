{
  "name": "angular-warmains",
  "version": "0.0.0",
  "description": "Character planning tool for World of Warcraft",
  "author": "Nasir Mohammad",
  "license": "MIT",
  "engines": {
    "node": "12.16.1",
    "npm": "6.14.2"
  },
  "main": "server.js",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "node dist/server/main",
    "serve": "ng serve --proxy-config proxy.config.json",
    "build": "ng build",
    "build:client:prod": "ng build --prod",
    "build:ssr:prod": "ng run angular-warmains:server:production",
    "dev:ssr": "ng run angular-warmains:serve-ssr",
    "prerender": "ng run angular-warmains:prerender",
    "heroku-postbuild": "npm run build:client:prod && npm run build:ssr:prod",
    "prettier:format": "prettier --print-width 120 --single-quote --trailing-comma none --write **/*.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.7",
    "@angular/cdk": "9.1.3",
    "@angular/common": "9.0.7",
    "@angular/compiler": "9.0.7",
