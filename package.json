    "body-parser": "^1.19.0",
    "core-js": "^3.6.4",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.5",
    "ng4-click-outside": "^1.0.1",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "typegoose": "^5.9.1",
    "zone.js": "~0.10.2"
